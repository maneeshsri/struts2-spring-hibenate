<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

   <constant name="struts.devMode" value="true" />

	<!-- Configuration for the default package. -->
	<package name="default" extends="struts-default" >	
		
		<action name="index" class="com.maneeshsri.sample.action.SearchJobAction" method="list">
			<result name="success">/jsp/search.jsp</result>
		</action>
		
	   <action name="search" class="com.maneeshsri.sample.action.SearchJobAction" method="search">
			<result name="success">/jsp/search.jsp</result>
		</action>
		
	</package>


</struts>